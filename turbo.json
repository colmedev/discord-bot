{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": ["**/.env.production", "**/.env.development", "**/.env.local"],
  "tasks": {
    "deploy-commands": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "CI",
    "API_URL",
    "PORT",
    "PREFIX",
    "DATABASE_URL",
    "DISCORD_APPLICATION_ID",
    "DISCORD_SERVER_ID",
    "DISCORD_TOKEN",
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "LEMON_SQUEEZY_URL",
    "LEMON_SQUEEZY_API_KEY"
  ]
}
